<mvc:View controllerName="sap.btp.ux410solving.controller.Detail"
  xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
  xmlns="sap.m"
  xmlns:l="sap.ui.layout"
  xmlns:f="sap.ui.layout.form"
  xmlns:core="sap.ui.core"
  xmlns:uxap="sap.uxap">
  <uxap:ObjectPageLayout>
    <uxap:headerContent>
      <VBox class="sapUiSmallMargin">
      <f:SimpleForm  editable="false" layout="ResponsiveGridLayout" title="{OrderID}" labelSpanXL="4" labelSpanL="3" labelSpanM="4" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="4" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="2" singleContainerFullSize="false">
        <f:content>
          <Text id="nameText" text="Order Detail view Details"/>
          <HBox>
              <Label text="OrderID" />
				      <Text text="{OrderID}" />
          </HBox>
        </f:content>
      </f:SimpleForm>
    </VBox>
    </uxap:headerContent>
    

  </uxap:ObjectPageLayout>
</mvc:View>
