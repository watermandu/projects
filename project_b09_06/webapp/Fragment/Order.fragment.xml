<c:FragmentDefinition xmlns:c="sap.ui.core"
  xmlns="sap.m"
  xmlns:t="sap.ui.table">


  <Dialog
id = "idDialog" beforeOpen="onBeforeOpen" title="Dialog Table" contentHeight="50%" contentWidth="40%" resizable="true" draggable="true">

    <content>
      <t:Table id='idOrderTable' rows="{/Orders}" selectionMode="MultiToggle" visibleRowCount="7" paste="onPaste" ariaLabelledBy="title">

        <!-- rows="{path: '/Orders',
              filters: [{
                path: 'EmployeeID',
                operator:'GE',
                value1: 4,
                value2: '' 
              }, {
                path: 'CustomerID',
                operator:'Contains',
                value1: 'R',
                value2: '' 
              }]
                }"
        -->
        <t:extension>
          <OverflowToolbar style="Clear">
            <Title id="title" text="Products"/>
          </OverflowToolbar>
        </t:extension>
        <t:columns>
          <t:Column width="11rem">
            <Label text="Product " />
            <t:template>
              <Text text="{OrderID}" wrapping="false" />
            </t:template>
          </t:Column>
          <t:Column width="11rem">
            <Label text="CustomerID " />
            <t:template>
              <Text text="{CustomerID}" wrapping="false" />
            </t:template>
          </t:Column>
          <t:Column width="11rem">
            <Label text="EmployeeID " />
            <t:template>
              <Text text="{EmployeeID}" wrapping="false" />
            </t:template>
          </t:Column>
        </t:columns>
      </t:Table>
    </content>
    <endButton >
      <Button text="close" press = "onClose"></Button>
    </endButton>
  </Dialog>


</c:FragmentDefinition>