<mvc:View controllerName="projectb0910.controller.Main"
  xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
  xmlns="sap.uxap"
  xmlns:m="sap.m">
  <ObjectPageLayout id="ObjectPageLayout" upperCaseAnchorBar="false">
    <headerTitle>
      <ObjectPageHeader objectTitle="배포 프로젝트">
      </ObjectPageHeader>
    </headerTitle>

    <headerContent>
      <m:Input showValueHelp="true" valueHelpRequest="onButton" width="20%"></m:Input>
      
    </headerContent>

    <sections>

      <ObjectPageSection titleUppercase="false" title="Section 5">
        <subSections>
          <ObjectPageSubSection titleUppercase="false">
            <blocks>
              
              <m:Table mode= "SingleSelectMaster" id="idTable" inset="false" items="{/SBOOKSet}" 
                width = "1000px">
                <m:columns>
                  <m:Column width="12em">
                    <m:Text text="Carrid" />
                  </m:Column>
                  <m:Column width="12em">
                    <m:Text text="Connid" />
                  </m:Column>
                  <m:Column width="12em">
                    <m:Text text="Fldate" />
                  </m:Column>
                  <m:Column width="12em">
                    <m:Text text="Bookid" />
                  </m:Column>
                </m:columns>
                <m:items>
                  <m:ColumnListItem vAlign="Middle">
                    <m:cells>
                      <m:Text text="{Carrid}" />
                      <m:Text text="{Connid}" />
                      <m:Text text="{Fldate}" />
                      <m:Text text="{Bookid}" />
                    </m:cells>
                  </m:ColumnListItem>
                </m:items>

              </m:Table>
            </blocks>
          </ObjectPageSubSection>
        </subSections>
      </ObjectPageSection>

      <ObjectPageSection titleUppercase="false" title="Section 6">
        <subSections>
          <ObjectPageSubSection titleUppercase="false">
            <blocks>
              <m:Image src="../img/cat.jpg" width="100px" />
              <m:Image src="../img/2.gif" width="100px" />
              <m:Image  id="idImg" width="100px" />
              <m:Image src="https://health.chosun.com/site/data/img_dir/2023/05/31/2023053102582_0.jpg" width="100px" />
              <m:Image src="https://blog.kakaocdn.net/dn/cdSkyP/btrsxjLUBi4/jQZSU0tIXPFMWEekWP3FA1/img.jpg" width="80%" />
            </blocks>
          </ObjectPageSubSection>
        </subSections>
      </ObjectPageSection>


    </sections>

  </ObjectPageLayout>
</mvc:View>
